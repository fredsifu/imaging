<div class="row">
  <div class="well col-lg-offset-2 col-lg-8">
    
    <div class="row">
      <div class="col-lg-11">
        
        <div class="row">
          <div class="col-lg-4">
            Name:
          </div>
          <div class="col-lg-8">
            <%= @patient.last_name + ', ' + @patient.first_name + ' ' + @patient.middle_name %>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-4">
            Insurance Number
          </div>
          <div class="col-lg-8">
            <%= @patient.insurance_number %>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-4">
            Complementary Insurance Number
          </div>
          <div class="col-lg-8">
            <%= @patient.complementary_insurance_number %>
          </div>
        </div>
    
        <!-- Contact Info -->
        
        <!-- Prescriptions follow up -->
          
      </div>
      <div class="col-lg-1">
        <%= link_to edit_patient_path(@patient), class: "btn btn-warning" do %>
          <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        <% end %>
      </div>
    </div>
    
  </div>
</div>
<%= link_to 'Return patients listing', patients_path, class: 'btn btn-warning' %>